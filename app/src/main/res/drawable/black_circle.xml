<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid
                android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:top="15dp"
        android:start="15dp"
        android:end="15dp"
        android:bottom="15dp">
        <shape android:shape="oval">
            <solid
                android:color="#555"/>
        </shape>
    </item>

</layer-list>